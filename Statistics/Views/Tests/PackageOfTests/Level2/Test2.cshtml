@{
    Layout = "~/Views/Tests/PackageOfTests/_LayoutPageTests.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <title>Level-2</title>
</head>
<body>
    <h1 align="center" style="color:green;">Test Level 2</h1>
    <br>
    <br>
    <br>
    <form id="Test2" method="post" runat="server">
        <div class="questions">

            <div class="question1">
                <br><b>What is the variance of these numbers 10.4,10.4,10.4,10.4,10.4?</b>
                <div class="answers1">
                    <div class="answer1">
                        <input type="radio" name="question-1-answers-A" id="1-A" value="A" />
                        <label for="question-1-answers-A">0</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-1-answers-B" id="1-B" value="B" />
                        <label for="question-1-answers-B">1</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-1-answers-C" id="1-C" value="C" />
                        <label for="question-1-answers-C">10.4</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-1-answers-D" id="1-D" value="D" />
                        <label for="question-1-answers-D">Not specified</label>
                    </div>
                </div>
            </div>


            <div class="question2">
                <br><b>A die is thrown twice the same time. What is the probability that the score is whatever except {6,6}?</b>
                <div class="answers2">
                    <div class="answer1">
                        <input type="radio" name="question-2-answers-A" id="2-A" value="A" />
                        <label for="question-2-answers-A">97%</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-2-answers-B" id="2-B" value="B" />
                        <label for="question-2-answers-B">92%</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-2-answers-C" id="3-C" value="C" />
                        <label for="question-2-answers-C">85%</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-2-answers-D" id="4-D" value="D" />
                        <label for="question-2-answers-D">60%</label>
                    </div>
                </div>
            </div>


            <div class="question3">
                <br><b>A fair coin is tossed two times. What is the probability of obtaining at least one Head?</b>
                <div class="answers3">
                    <div class="answer1">
                        <input type="radio" name="question-3-answers-A" id="3-A" value="A" />
                        <label for="question-3-answers-A">3/4</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-3-answers-B" id="3-B" value="B" />
                        <label for="question-3-answers-B">1/2</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-3-answers-C" id="3-C" value="C" />
                        <label for="question-3-answers-C">1/3</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-3-answers-D" id="3-D" value="D" />
                        <label for="question-2-answers-D">1/8</label>
                    </div>
                </div>
            </div>


            <div class="question4">
                <br><b>What is the median of the following set of numbers? {12,8,13,4,7,6,3,3,15}</b>
                <div class="answers4">
                    <div class="answer1">
                        <input type="radio" name="question-4-answers-A" id="4-A" value="A" />
                        <label for="question-2-answers-A">7</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-4-answers-B" id="4-B" value="B" />
                        <label for="question-2-answers-B">8</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-4-answers-C" id="4-C" value="C" />
                        <label for="question-2-answers-C">10</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-4-answers-D" id="4-D" value="D" />
                        <label for="question-2-answers-D">6</label>
                    </div>
                </div>
            </div>

            <div class="question5">
                <br><b>C = { x ∈Z | -1 ≤ x ≤ 1},  F = { x ∈Z | x ≤ -1}. What is C ∩ F?</b>
                <div class="answers5">
                    <div class="answer1">
                        <input type="radio" name="question-5-answers-A" id="5-A" value="A" />
                        <label for="question-5-answers-A">{-1}</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-5-answers-B" id="5-B" value="B" />
                        <label for="question-5-answers-B">{-1,0,1}</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-5-answers-C" id="5-C" value="C" />
                        <label for="question-5-answers-C">&empty;</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-5-answers-D" id="5-D" value="D" />
                        <label for="question-5-answers-D">All Z ecxept [-1,1]</label>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <br>
        <input type="submit" onclick="validateTest();" value="submit" name="submit" class="button" /> 
        <input type="reset" value="Reset" class="button" />
    </form>
</body>
</html>
