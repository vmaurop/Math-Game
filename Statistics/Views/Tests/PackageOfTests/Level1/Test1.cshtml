@{
    Layout = "~/Views/Tests/PackageOfTests/_LayoutPageTests.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <title>Level-1</title>
</head>
<body>

    <h1 align="center" style="color:green;">Tets Level 1</h1>
    <br>
    <br>
    <br>
    <form id="Test1" method="post" runat="server">
        <div class="questions">

            <div class="question1">
                <br><b>What is the median of the numbers 4, 2, 11, 6, 2 ?</b>
                <div class="answers1">
                    <div class="answer1">
                        <input type="radio" name="question-1-answers-A" id="1-A" value="A" />
                        <label for="question-1-answers-A">4</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-1-answers-B" id="1-B" value="B" />
                        <label for="question-1-answers-B">11</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-1-answers-C" id="1-C" value="C" />
                        <label for="question-1-answers-C">5</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-1-answers-D" id="1-D" value="D" />
                        <label for="question-1-answers-D">2</label>
                    </div>
                </div>
            </div>


            <div class="question2">
                <br><b>What is the mean of the numbers 8, 2, 14, 8?</b>
                <div class="answers2">
                    <div class="answer1">
                        <input type="radio" name="question-2-answers-A" id="2-A" value="A" />
                        <label for="question-2-answers-A">8</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-2-answers-B" id="2-B" value="B" />
                        <label for="question-2-answers-B">2</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-2-answers-C" id="3-C" value="C" />
                        <label for="question-2-answers-C">7.5</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-2-answers-D" id="4-D" value="D" />
                        <label for="question-2-answers-D">14</label>
                    </div>
                </div>
            </div>


            <div class="question3">
                <br><b>A die is thrown once. What is the probability that the score is a factor of 6?</b>
                <div class="answers3">
                    <div class="answer1">
                        <input type="radio" name="question-3-answers-A" id="3-A" value="A" />
                        <label for="question-3-answers-A">2/3</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-3-answers-B" id="3-B" value="B" />
                        <label for="question-3-answers-B">1/2</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-3-answers-C" id="3-C" value="C" />
                        <label for="question-3-answers-C">1/6</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-3-answers-D" id="3-D" value="D" />
                        <label for="question-2-answers-D">1/3</label>
                    </div>
                </div>
            </div>


            <div class="question4">
                <br><b>
                    Ten friends scored the following marks in their end-of-year math exam:
                    23%, 37%, 45%, 49%, 56%, 63%, 63%, 70%, 72% and 82%<br>
                    What was the standard deviation of their marks?
                </b>
                <div class="answers4">
                    <div class="answer1">
                        <input type="radio" name="question-4-answers-A" id="4-A" value="A" />
                        <label for="question-2-answers-A">16.9%</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-4-answers-B" id="4-B" value="B" />
                        <label for="question-2-answers-B">21%</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-4-answers-C" id="4-C" value="C" />
                        <label for="question-2-answers-C">22%</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-4-answers-D" id="4-D" value="D" />
                        <label for="question-2-answers-D">14%</label>
                    </div>
                </div>
            </div>

            <div class="question5">
                <br><b>What is the population standard deviation for the numbers: 75, 83, 96, 100, 121 and 125?</b>
                <div class="answers5">
                    <div class="answer1">
                        <input type="radio" name="question-5-answers-A" id="5-A" value="A" />
                        <label for="question-5-answers-A">18.2</label>
                    </div>
                    <div class="answer2">
                        <input type="radio" name="question-5-answers-B" id="5-B" value="B" />
                        <label for="question-5-answers-B">17.1</label>
                    </div>
                    <div class="answer3">
                        <input type="radio" name="question-5-answers-C" id="5-C" value="C" />
                        <label for="question-5-answers-C">15.3</label>
                    </div>
                    <div class="answer4">
                        <input type="radio" name="question-5-answers-D" id="5-D" value="D" />
                        <label for="question-5-answers-D">19</label>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <br>
        <input type="submit" onclick="validateTest();" value="submit" name="submit" class="button" /> 
        <input type="reset" value="Reset" class="button" />
   
    </form>
 


</body>
</html>
